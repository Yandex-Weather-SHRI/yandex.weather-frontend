{
  "name": "yandex.weather",
  "version": "0.1.0",
  "scripts": {
    "start": "babel-node server/server.js --presets es2015",
    "postinstall": "npm run build:prod",        
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint src",
    "lint:js-fix": "eslint src --fix",
    "lint:css": "stylelint \"src/**/*.js\"",
    "dev": "webpack-dev-server",
    "build": "webpack",
    "copy-manifest": "cp ./src/manifest.json ./build",
    "build:prod": "NODE_ENV=production npm run build -- -p && npm run copy-manifest",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "prepush": "npm run lint",
    "test": "npm run lint && npm run test:jest",
    "test:jest": "NODE_ENV=test NODE_PATH=src jest",
    "test:jest-watch": "npm run test:jest -- --watch"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-backgrounds": "^3.0.0",
    "@storybook/addon-info": "^3.2.6",
    "@storybook/addon-options": "^3.2.6",
    "@storybook/react": "^3.2.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-minify-dead-code-elimination": "^0.2.0",
    "babel-plugin-ramda": "^1.4.3",
    "babel-plugin-styled-name": "^1.0.0",
    "babel-plugin-transform-inline-consecutive-adds": "^0.2.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.5",
    "eslint": "^4.5.0",
    "eslint-config-atomix-react": "^2.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "prop-types": "^15.5.10",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-svg-loader": "^1.1.1",
    "redux-logger": "^3.0.6",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-processor-styled-components": "^0.3.0",
    "babel-jest": "^21.0.2",
    "webpack": "^3.5.5",
    "jest": "^21.0.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0",
    "favicons-webpack-plugin": "0.0.7"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^15.6.1",
    "react-id-swiper": "^1.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.2",
    "react-share": "^1.16.0",
    "redux": "^3.7.2",
    "redux-act": "^1.3.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "styled-components": "^2.1.2"
  }
}
